<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>PayloadContent</key>
        <dict>
            <key>URL</key>
            <string><%= retrieve_udid_index_url %></string>
            <key>DeviceAttributes</key>
            <array>
                <string>UDID</string>
                <string>IMEI</string>
                <string>SERIAL</string>
                <string>VERSION</string>
                <string>PRODUCT</string>
            </array>
        </dict>
        <key>PayloadOrganization</key>
        <string><%= t('udid.install.organization') %></string>
        <key>PayloadDisplayName</key>
        <string><%= t('udid.install.title') %></string>
        <key>PayloadDescription</key>
        <string><%= t('udid.install.description', name: Setting.site_title, url: root_url) %></string>
        <key>PayloadIdentifier</key>
        <string>im.ews.zealot.profile-service</string>
        <key>PayloadType</key>
        <string>Profile Service</string>
        <key>PayloadVersion</key>
        <integer>1</integer>
        <key>PayloadUUID</key>
        <string><%= @udid %></string>
    </dict>
</plist>
